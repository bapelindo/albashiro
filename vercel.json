{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.4",
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/chat/(.*)",
            "dest": "/api/index.php?url=chat/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php?url=$1"
        }
    ]
}